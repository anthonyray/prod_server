{{>header}}
    {{> menu}}
    <div class="text-center">
    <h1>{{ song.title }}</h1>
    <h4 class="text-muted">by <a href="/artist/{{song.artist.id}}">{{song.artist.name}}</a></h4>
    </div>
    <br>
    <br>
    <ul>
      <li>Artist : {{song.artist.name}}</li>
      <li>Producer : {{song.producer.name}}</li>
      <li>Album : {{song.album}}</li>
      <li>upvotes : {{song.upvotes}}</li>
      <li>media : <i>{{song.media.source}}</i> <b>{{song.media.source}}</b>
      <li>Annotations :
        <ul>
          {{#song.annotations}}
          <li>{{type}} - {{description}}</li>
          {{/song.annotations}}
        </ul>
      <li>Submitted by : {{submitter.local.email}}</li>
    </ul>
{{>footer}}
