{{>header}}
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          {{> menu}}
          <div class="text-center">
            <h1> Explore the knowledge </h1>
            <h4 class="text-muted"> Find answers from the community of producers and DJs</h4>
          </div>
          <br>
          <br>
          <nav>
            <ul class="nav nav-pills nav-justified">
              <li class="active"><a href="#">Best</a></li>
              <li><a href="#">Trending</a></li>
              <li><a href="#">Most Discussed</a></li>
            </ul>
          </nav>
          <br>
          <br>
            {{# songs}}
            <div class="media">
              <a class="media-left pull-left" href="#">
                <img class="img img-circle" src="images/placeholder.gif">
              </a>
              <div class="media-body">
                <h4 class="media-heading"><a href="song/{{_id}}"> {{title}} - {{artist.name}}</a></h4>
                <p>Produced by</p>

              </div>
            </div>
            <br>

            {{/songs}}

            <nav>
              <ul class="pager">
                <li><a href="#">Next <span aria-hidden="true">&rarr;</span></a></li>

              </ul>
            </nav>
        </div>
    </div>
  </div>
{{>footer}}
